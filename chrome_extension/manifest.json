{
  "manifest_version": 2,

  "name": "WebPeb",
  "description": "Chrome extension part of WebPeb",
  "version": "1.0",

  "background": { 
	  "page": "background.html"
  }, 

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    "jquery-1.10.2.min.js",
    "jquery-1.10.2.min.map",
    "jquery.js"
  ],

  "permissions": [
    "tabs",
    "http://*/*", 
    "https://*/*"
  ]
}
